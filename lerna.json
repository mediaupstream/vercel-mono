{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": "main",
      "exact": true,
      "conventionalCommits": true,
      "forcePublish": true,
      "yes": true,
      "message": "chore(release): publish %s"
    }
  },
  "packages": [
    "packages/*"
  ],
  "version": "1.6.0"
}
